<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
  id="TyckTill" version="2.0">
  
  <portlet>
    <description>Portlet for Tyck till application using Pivotal Tracker</description>
    <portlet-name>IncidentReportPortletPivotal</portlet-name>
    <display-name>Tyck till Pivotal</display-name>
    <portlet-class>se.vgr.incidentreport.IncidentReportPortlet</portlet-class>
    
    <init-param>
      <name>ApplicationName</name>
      <value>Portal_project</value>
    </init-param>
    <init-param>
      <name>TyckTillErrorFormURL</name>
      <value>http://tycktill.vgregion.se/tyck-till-core-bc-module-web/xforms-jsp/index.jsp</value>
    </init-param>
    <init-param>
      <name>TyckTillReportMethod</name>
      <value>pivotaltracker</value>
    </init-param>
    <init-param>
      <name>TyckTillReportEmail</name>
      <value>tycktill@vgregion.se</value>
    </init-param>

    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
    </supports>
    <resource-bundle>IncidentReport</resource-bundle>
    
    <portlet-info>
      <title>Tyck till Pivotal</title>
      <short-title>Tyck till Pivotal</short-title>
    </portlet-info>
  </portlet>
