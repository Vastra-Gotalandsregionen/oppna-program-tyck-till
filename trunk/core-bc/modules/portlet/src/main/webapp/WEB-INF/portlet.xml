<?xml version="1.0" encoding="UTF-8"?>
<portlet-app xmlns="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd http://java.sun.com/xml/ns/portlet/portlet-app_2_0.xsd"
  id="TyckTill" version="2.0">
  <portlet>
    <portlet-name>IncidentReportPortlet</portlet-name>
    <portlet-class>se.vgr.incidentreport.IncidentReportPortlet</portlet-class>
     <init-param>
      <name>tyck_till_form_url</name>
      <value>http://webbisar.vgregion.se/tyck-till/xforms-jsp/index.jsp</value> <!-- Test/dev server -->
     <!--  <value>http://localhost:8082/tyck-till-core-bc-module-web/xforms-jsp/index.jsp</value>--><!-- Local env -->
     </init-param>
    
    <expiration-cache>0</expiration-cache>
    <supports>
      <mime-type>text/html</mime-type>
      <portlet-mode>VIEW</portlet-mode>
    </supports>
    <resource-bundle>IncidentReport</resource-bundle>
  </portlet>

</portlet-app>
